{
    "name": "db",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "lib/db/src",
    "projectType": "library",
    "buildable": true,
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": [
                "{options.outputPath}"
            ],
            "dependsOn": [
                "generate"
            ],
            "options": {
                "outputPath": "dist/lib/db",
                "main": "lib/db/src/index.ts",
                "tsConfig": "lib/db/tsconfig.lib.json"
            }
        },
        "migrate-reset": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm dlx prisma migrate reset",
                "cwd": "lib/db"
            }
        },
        "generate": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm dlx prisma generate",
                "cwd": "lib/db"
            }
        },
        "migrate": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm  prisma migrate dev ",
                "cwd": "lib/db"
            }
        },
        "studio": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm dlx prisma studio",
                "cwd": "lib/db"
            }
        },
        "db-push": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm dlx prisma db push --accept-data-loss",
                "cwd": "lib/db"
            }
        }
    },
    "tags": []
}