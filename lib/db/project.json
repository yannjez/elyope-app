{
    "name": "db",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "lib/db/src",
    "projectType": "library",
    "buildable": true,
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": [
                "{options.outputPath}"
            ],
            "dependsOn": [
                "generate"
            ],
            "options": {
                "outputPath": "dist/lib/db",
                "main": "lib/db/src/index.ts",
                "tsConfig": "lib/db/tsconfig.lib.json"
            }
        },
        "migrate-reset": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm  prisma migrate reset",
                "cwd": "lib/db"
            }
        },
        "generate": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm  prisma generate",
                "cwd": "lib/db"
            }
        },
        "migrate-deploy": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm  prisma migrate deploy",
                "cwd": "lib/db"
            }
        },
        "migrate": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm  prisma migrate dev ",
                "cwd": "lib/db"
            }
        },
        "studio": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm  prisma studio",
                "cwd": "lib/db"
            }
        },
        "db-push": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm  prisma db push --accept-data-loss",
                "cwd": "lib/db"
            }
        },
        "seed": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm ts-node prisma/seed.ts",
                "cwd": "lib/db"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "lib/db/**/*.{ts,tsx,js,jsx}"
                ]
            }
        },
        "typecheck": {
            "executor": "@nx/js:tsc",
            "options": {
                "main": "lib/db/src/index.ts",
                "tsConfig": "lib/db/tsconfig.lib.json",
                "outputPath": "dist/lib/db",
                "noEmit": true,
                "verbose": true
            },
            "dependsOn": [
                "^typecheck"
            ]
        }
    },
    "tags": []
}